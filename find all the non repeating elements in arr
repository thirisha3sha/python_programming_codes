# find all the non repeating elements in arr
def non_repeating(arr):
    freq={}
    for i in arr:
        if i in freq:
            freq[i]+=1
        else:
            freq[i]=1
    result=[]
    for i in arr:
        if freq[i]==1:
            result.append(i)    
    return result
print(non_repeating([4,2,1,2,3,4,5,1]))  # Output: [3, 5]  dict={4:2,2:2,1:2,3:1,5:1}
